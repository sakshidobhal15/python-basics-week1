
# parameters_return.py
# Demonstrates parameters and return values

def add(a, b):
    return a + b

result = add(10, 20)
print("Sum:", result)


def square(num):
    return num * num

print("Square:", square(5))


def student_details(name, age):
    return name, age

n, a = student_details("Sakshi", 20)
print("Name:", n)
print("Age:", a)
